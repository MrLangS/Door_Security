<view class='list' wx:for='{{list}}' wx:key='{{index}}' data-id='{{item._id}}'>
  <image class='poster' src='{{item.img? item.img:"/images/empty_avatar_user.png"}}'></image>
  <view class='info'>
    <view class='title'>{{item.deviceName}}</view>
    <view class='author'>{{item.peopleName}}</view>
    <view class='others'>
      <view>
        <text class='isFinished'>出入时间 · </text>
        <text>{{item.time}}</text>
      </view>
    </view>
  </view>
  <view class='price' >{{item.role}}</view>
</view>