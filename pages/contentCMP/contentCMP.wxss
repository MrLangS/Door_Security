.wrapper {
  /* padding-top: 20rpx; */
  margin-top: 200rpx;
  padding-bottom: 100rpx;
}

.modalpicker{
  position: relative;
  width: 100%; 
  height: 300px;
}
.add-depart{
  display: flex;
  flex-direction: row;
}
.left-image{
  margin-right: 10rpx !important;
}

.search-peo{
  background: #fff;
  position: fixed;
  width: 100%;
  z-index: 100;
  right: 0;
  top: 0;
  left: 0;
  color: #888888;
}
.search-con{
  background: #F4F6F9;
  display: flex;
  direction: row;
  height: 70rpx;
  justify-content: start;
  align-items: center;
  margin: 10rpx 40rpx;
}
.final:last-child{
  color: #888888;
}

.search-peo image{
  width: 28rpx;
  height: 28rpx;
  margin: 0 20rpx;
}
.root{
  /* height: 90rpx; */
  line-height: 90rpx;
  color: #3281ff;
  padding: 0 40rpx;
}

.new{
  background: #fff;
  height: 90rpx;
  position: fixed;
  width: 100%;
  z-index: 100;
  display: flex;
  justify-content: space-around;
  align-items: center;
  right: 0;
  bottom: 0;
  left: 0;
  color: white;
  background: #3281ff;
}
.bottom{
  height: 90rpx;
  line-height: 90rpx;
  width: 50%;
  text-align: center;
}
.bottombut{
  -webkit-font-smoothing: antialiased;
  font-family: "PingHei","Helvetica Neue","Helvetica","Arial","Verdana","sans-serif";
  font-size: 28rpx;
  color: #fff;
}
.hover-cla {
  background: #f2f2f2 !important;
  color: #3281ff;
}

.content{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(170px);
  transform: translateX(170px);
  margin-left: -170px;
}
.content-staff{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(240px);
  transform: translateX(240px);
  margin-left: -240px;
}
.items .item .more image{
  width: 20rpx;
  height: 40rpx;
  margin-right: 0;
}
.more {
  display: flex;
  align-items: center;
}
.staff-type {
  background-color: #3281ff;
  height: 90rpx;
  width: 100px;
  display: flex;
  position: relative;
  /* margin-left: 40rpx; */
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  left: 40rpx;
  -webkit-transform: translateX(100px);
  transform: translateX(100px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.admin-type{
  background-color: #3281ff;
  height: 90rpx;
  width: 100px;
  display: flex;
  position: relative;
  /* margin-left: 40rpx; */
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  left: 40rpx;
  -webkit-transform: translateX(170px);
  transform: translateX(170px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.del {
  background-color: orangered;
  height: 90rpx;
  width: 70px;
  display: flex;
  position: relative;
  /* margin-left: 40rpx; */
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  left: 40rpx;
  -webkit-transform: translateX(70px);
  transform: translateX(70px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.code-admin {
  background-color: #05bf5c;
  position: relative;
  left: 40rpx;
  height: 180rpx;
  width: 70px;
  display: flex;
  margin-right: 0rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(170px);
  transform: translateX(170px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.code {
  background-color: #05bf5c;
  position: relative;
  left: 40rpx;
  height: 180rpx;
  width: 70px;
  display: flex;
  margin-right: 0rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(240px);
  transform: translateX(240px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
.touch-move-active .content,.touch-move-active .content-staff, .touch-move-active .del,.touch-move-active .admin-type, .touch-move-active .staff-type, .touch-move-active .code,.touch-move-active .code-admin {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}
.rightImg{
  position: absolute;
  right: 40rpx;
  width: 150rpx;
  height: 150rpx;
}
.logo-height{
  height: 150rpx;
  padding: 0 40rpx;
  border-bottom: 1rpx solid #efefef;
  display: flex;
  justify-content: space-between;
  align-items: center;
  overflow: hidden;
}