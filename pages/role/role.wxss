.content {
  background: #282923;
  position: fixed;
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.logo_bg {
  display: flex;
  flex-direction: row;
  justify-content: center;
  width: 100%;
  height: 400rpx;
}

.logo_bg .logo {
  width: 200rpx;
  height: 166rpx;
  margin-top: 100rpx;
}

.admin-box{
  width: 85%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
  margin-top: -100rpx;
  background: #3281ff;
  color: white;
}

.box-title {
  position: fixed;
  height: 90rpx;
  line-height: 90rpx;
  margin-top: 155rpx;
  right: 85rpx;
  /* top: 100rpx; */
}

.staff-box{
  width: 85%;
  height: 300rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 10px;
  box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.05);
  margin-top: 50rpx;
  background: #36c8c3;
  color: white;
}